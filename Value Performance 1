import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner Sc=new Scanner(System.in);
        int num = Sc.nextInt();
        float num2 = Sc.nextFloat();
        System.out.println(num);
        System.out.printf("%.2f",num2);
        
    }
}
