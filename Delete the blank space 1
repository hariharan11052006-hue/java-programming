import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner Sc=new Scanner(System.in);
        String S =Sc.nextLine();
        String str = S.replace(" ","");
        System.out.println(str);
    }
}
